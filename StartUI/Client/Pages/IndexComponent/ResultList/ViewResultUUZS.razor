@using UUZSDataProto.V1;
@using FiltersGSOProto.V1;
@inherits FiltrInherits<CreateResultsListCacheAso>
@if (!IsPageLoad)
{
    <FiltrInput AddItemFiltr="AddItemFiltr" RemoveItemsFiltr="RemoveItemsFiltr" Items="FiltrItems" Hints="HintItems" PlaceHolder="@PlaceHolder" />
    <TableVirtualize @ref="table" TItem="CLVResult" Provider="GetProvider" IsSetFocus="false">
        <Tbody Context="item">
        <td>@item.SitName</td>
        <td>@item.DevName</td>
        <td>@item.GroupName</td>
        <td>@item.ResultName</td>
        <td>@item.CmdTime?.ToDateTime().ToLocalTime().ToString("T")</td>
        <td>@item.ConnParam</td>
        <td>@item.CallName</td>
        </Tbody>
        <TSticky>
            <ResultNotifySticky />
        </TSticky>
    </TableVirtualize>
}